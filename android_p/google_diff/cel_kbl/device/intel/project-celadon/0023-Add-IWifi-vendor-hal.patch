From 20a67b8afd782246ca02e36ff4fdf20ea0170632 Mon Sep 17 00:00:00 2001
From: Jeevaka Prabu Badrappan <jeevaka.badrappan@intel.com>
Date: Fri, 1 Feb 2019 10:14:31 +0530
Subject: [PATCH] Add IWifi vendor hal

Tracked-On:
Signed-off-by: Jeevaka Prabu Badrappan <jeevaka.badrappan@intel.com>
---
 manifest.xml | 9 +++++++++
 1 file changed, 9 insertions(+)

diff --git a/manifest.xml b/manifest.xml
index b994cd6..259856d 100644
--- a/manifest.xml
+++ b/manifest.xml
@@ -150,6 +150,15 @@
             <instance>default</instance>
         </interface>
     </hal>
+    <hal format="hidl">
+        <name>android.hardware.wifi</name>
+        <transport>hwbinder</transport>
+        <version>1.2</version>
+        <interface>
+            <name>IWifi</name>
+            <instance>default</instance>
+        </interface>
+    </hal>
     <hal format="hidl">
         <name>android.hardware.wifi.hostapd</name>
         <transport>hwbinder</transport>
